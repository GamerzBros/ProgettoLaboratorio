      <Button fx:id="nuovo_vaccinato" layoutX="41.0" layoutY="207.0" mnemonicParsing="false" onAction="#onNewVaccinate" prefHeight="40.0" prefWidth="160.0" style="-fx-background-color: white; -fx-background-radius: 8px;" text="Nuovo Vaccinato" textFill="black">
         <font>
            <Font size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="nuovo_centro_ui" layoutX="297.0" layoutY="207.0" mnemonicParsing="false" onAction="#onNuovoCentroSelected" prefHeight="40.0" prefWidth="160.0" style="-fx-background-color: white; -fx-background-radius: 8px;" text="Nuovo Centro" textFill="black">
         <font>
            <Font name="System" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ImageView fitHeight="134.0" fitWidth="160.0" layoutX="297.0" layoutY="57.0" pickOnBounds="true" preserveRatio="false">
         <image>
            <Image url="@/centrivaccinali/ospedale.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="120.0" fitWidth="145.0" layoutX="48.0" layoutY="57.0" pickOnBounds="true" preserveRatio="false">
         <image>
            <Image url="@/centrivaccinali/person.png" />
         </image></ImageView>
      <!--<ImageView fitHeight="22.0" fitWidth="25.0" layoutX="2.0" layoutY="4.5" onMouseClicked="#goBackFromOpzioniOperatore" pickOnBounds="true" preserveRatio="false" style="-fx-background-color: black">
         <Image url="@/centrivaccinali/goBackIcon.png" />
      </ImageView>-->
      <Button layoutX="2.0" layoutY="4.5" onMouseClicked="#goBackToSelectionUI" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="22.0" fitWidth="25.0" preserveRatio="false">
               <Image url="@/centrivaccinali/goBackIcon.png" />
            </ImageView>
         </graphic>
      </Button>
   </children>
