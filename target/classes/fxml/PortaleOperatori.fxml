<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="centrivaccinali.PortaleOperatori">
   <children>
      <ImageView fitHeight="300" fitWidth="500" preserveRatio="false">
         <Image url="@/centrivaccinali/medici.png" />
      </ImageView>
      <Rectangle arcHeight="25.0" arcWidth="25.0" fill="#1a73e8" height="227.0" layoutX="30.0" layoutY="36.0" opacity="0.75" style="-fx-background-radius: 200;" width="182.0" />
