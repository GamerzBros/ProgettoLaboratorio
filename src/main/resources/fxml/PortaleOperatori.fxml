<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" >
   <children>
      <ImageView fitHeight="300" fitWidth="500" preserveRatio="false">
         <Image url="@/centrivaccinali/medici.png" />
      </ImageView>
      <Rectangle arcHeight="25.0" arcWidth="25.0" fill="#1a73e8" height="227.0" layoutX="30.0" layoutY="36.0" opacity="0.85" style="-fx-background-radius: 200;" width="182.0" />
      <Rectangle arcHeight="25.0" arcWidth="25.0" fill="#1a73e8" height="227.0" layoutX="286.0" layoutY="36.0" opacity="0.85" style="-fx-border-radius: 200;" width="182.0" />
      <Button fx:id="nuovo_vaccinato" layoutX="41.0" layoutY="207.0" mnemonicParsing="false" onAction="#onNewVaccinate" prefHeight="40.0" prefWidth="160.0" style="-fx-background-color: white; -fx-background-radius: 8px;" text="Nuovo Vaccinato" textFill="black">
         <font>
            <Font name="System" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="nuovo_centro_ui" layoutX="297.0" layoutY="207.0" mnemonicParsing="false" onAction="#onNuovoCentroSelected" prefHeight="40.0" prefWidth="160.0" style="-fx-background-color: white; -fx-background-radius: 8px;" text="Nuovo Centro" textFill="black">
         <font>
            <Font name="System" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ImageView fitHeight="180.0" fitWidth="150.0" layoutX="302" layoutY="14.0" pickOnBounds="true">
         <image>
            <Image url="@/centrivaccinali/ospedale.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="170.0" fitWidth="170.0" layoutX="35.5" layoutY="32.0" pickOnBounds="true">
         <image>
            <Image url="@/centrivaccinali/person.png" />
         </image>
      </ImageView>
      <Button layoutX="2.0" layoutY="4.5" onMouseClicked="#goBackToSelectionUI" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="22.0" fitWidth="25.0" preserveRatio="false">
               <Image url="@/common/goBackIcon.png" />
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
