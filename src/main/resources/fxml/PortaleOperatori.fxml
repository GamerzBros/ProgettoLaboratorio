<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="300" fitWidth="500" preserveRatio="false" style="-fx-background-color: transparent">
         <Image url="@/centrivaccinali/medici.png" />
      </ImageView>
      <Button layoutX="2.0" layoutY="4.5" onMouseClicked="#goBackToSelectionUI" styleClass="backButton">
         <graphic>
            <ImageView fitHeight="22.0" fitWidth="25.0" preserveRatio="false">
               <Image url="@/common/goBackIcon.png" />
            </ImageView>
         </graphic>
      </Button>
      <AnchorPane layoutX="30.0" layoutY="36.0" prefHeight="200.0" prefWidth="200.0" style="-fx-background-radius: 18;" styleClass="rectangleChip">
         <children>
            <Rectangle arcHeight="25.0" arcWidth="25.0" fill="#1a73e8" height="227.0" opacity="0.85" style="-fx-background-radius: 200;" width="182.0" />
            <Button fx:id="nuovo_vaccinato" layoutX="10.0" layoutY="172.0" mnemonicParsing="false" onAction="#onNewVaccinate" prefHeight="40.0" prefWidth="160.0" styleClass="buttonChip" text="Nuovo Vaccinato" textFill="black">
               <font>
                  <Font name="System" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <ImageView fitHeight="170.0" fitWidth="170.0" layoutX="5.0" pickOnBounds="true">
               <image>
                  <Image url="@/centrivaccinali/person.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="286.0" layoutY="36.0" prefHeight="200.0" prefWidth="200.0" style="-fx-background-radius: 18;" styleClass="rectangleChip">
         <children>
            <Rectangle arcHeight="25.0" arcWidth="25.0" fill="#1a73e8" height="227.0" opacity="0.85" style="-fx-border-radius: 200;" width="182.0" />
            <ImageView fitHeight="180.0" fitWidth="150.0" layoutX="17.0" layoutY="-20.0" pickOnBounds="true">
               <image>
                  <Image url="@/centrivaccinali/ospedale.png" />
               </image>
            </ImageView>
            <Button fx:id="nuovo_centro_ui" layoutX="10.0" layoutY="172.0" mnemonicParsing="false" onAction="#onNuovoCentroSelected" prefHeight="40.0" prefWidth="160.0" styleClass="buttonChip" text="Nuovo Centro" textFill="black">
               <font>
                  <Font name="System" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children></AnchorPane>
   </children>
   <stylesheets>
      <URL value="@/style/style.css" />
   </stylesheets>
</AnchorPane>
